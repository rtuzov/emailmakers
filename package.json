{
  "name": "email-makers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:reset": "docker-compose down -v && docker-compose up -d && sleep 5 && npm run db:migrate",
    "db:setup-workers": "npx tsx scripts/create-worker-nodes-table.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:validators": "jest __tests__/validators --coverage",
    "test:validators:watch": "jest __tests__/validators --watch",
    "test:integration": "jest __tests__/validators/integration-workflow.test.ts",
    "check:api-keys": "node scripts/check-api-keys.js",
    "setup": "npm run check:api-keys && npm run type-check",
    "tsc:strict": "tsc -p tsconfig.strict.json --noEmit",
    "build:bundle": "tsup",
    "docs:generate": "ts-node scripts/generate-openapi.ts"
  },
