<mjml>
  <mj-head>
    <mj-title>{{campaign_name}} - Carousel Multi-Destination Campaign</mj-title>
    <mj-preview>{{preview_text}} –ò—Å—Å–ª–µ–¥—É–π—Ç–µ –º–Ω–æ–∂–µ—Å—Ç–≤–æ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–π</mj-preview>
    <mj-attributes>
      <mj-all font-family="Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Arial, sans-serif" />
      <mj-text font-size="16px" color="#1E293B" line-height="1.6" />
      <mj-button background-color="#2563EB" color="white" font-size="16px" font-weight="600" border-radius="8px" />
      <mj-section background-color="white" />
    </mj-attributes>
    <mj-style inline="inline">
      /* Dark mode support */
      @media (prefers-color-scheme: dark) {
        .dark-mode-bg { background-color: #111827 !important; }
        .dark-mode-text { color: #F3F4F6 !important; }
        .dark-mode-card { background-color: #1F2937 !important; }
      }
      
      /* Carousel layout styles */
      .carousel-container {
        background: white;
        border-radius: 16px;
        overflow: hidden;
        box-shadow: 0 8px 32px rgba(0, 0, 0, 0.1);
      }
      
      .carousel-item {
        border-radius: 12px;
        overflow: hidden;
        box-shadow: 0 4px 16px rgba(0, 0, 0, 0.08);
        transition: transform 0.2s ease, box-shadow 0.2s ease;
        background: white;
        margin: 0 8px;
        min-height: 320px;
        position: relative;
      }
      
      .carousel-item:hover {
        transform: translateY(-3px);
        box-shadow: 0 8px 32px rgba(0, 0, 0, 0.15);
      }
      
      .featured-destination {
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
        color: white;
        box-shadow: 0 12px 40px rgba(102, 126, 234, 0.3);
      }
      
      .featured-overlay {
        background: linear-gradient(
          135deg, 
          rgba(0, 0, 0, 0.2) 0%, 
          rgba(0, 0, 0, 0.5) 100%
        );
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        z-index: 1;
      }
      
      .featured-content {
        position: relative;
        z-index: 2;
      }
      
      .price-tag {
        background: linear-gradient(135deg, #10B981, #059669);
        color: white;
        padding: 6px 12px;
        border-radius: 16px;
        font-size: 12px;
        font-weight: 600;
        display: inline-block;
        position: absolute;
        top: 12px;
        right: 12px;
        z-index: 3;
        box-shadow: 0 2px 8px rgba(16, 185, 129, 0.3);
      }
      
      .price-tag-featured {
        background: linear-gradient(135deg, #F59E0B, #D97706);
        font-size: 14px;
        padding: 8px 16px;
        border-radius: 20px;
      }
      
      .destination-rank {
        background: rgba(37, 99, 235, 0.9);
        color: white;
        width: 28px;
        height: 28px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 12px;
        font-weight: 700;
        position: absolute;
        top: 12px;
        left: 12px;
        z-index: 3;
        box-shadow: 0 2px 8px rgba(37, 99, 235, 0.4);
      }
      
      .carousel-navigation {
        background: #F8FAFC;
        padding: 15px;
        text-align: center;
        border-radius: 0 0 16px 16px;
      }
      
      .nav-dot {
        width: 8px;
        height: 8px;
        background: #CBD5E1;
        border-radius: 50%;
        display: inline-block;
        margin: 0 4px;
        transition: background-color 0.2s ease;
      }
      
      .nav-dot.active {
        background: #2563EB;
        transform: scale(1.2);
      }
      
      .highlight-compact {
        background-color: rgba(248, 250, 252, 0.8);
        border-left: 3px solid #2563EB;
        padding: 6px 10px;
        margin: 3px 0;
        border-radius: 4px;
        font-size: 12px;
        line-height: 1.3;
      }
      
      .highlight-featured {
        background-color: rgba(255, 255, 255, 0.15);
        border-left: 3px solid #F59E0B;
        padding: 8px 12px;
        margin: 6px 0;
        border-radius: 6px;
        font-size: 13px;
        color: rgba(255, 255, 255, 0.9);
        backdrop-filter: blur(4px);
      }
      
      .cta-compact {
        background: linear-gradient(135deg, #2563EB, #1D4ED8);
        border-radius: 6px;
        font-weight: 600;
        font-size: 12px;
        padding: 8px 16px;
      }
      
      .cta-featured {
        background: linear-gradient(135deg, #FFFFFF, #F8FAFC);
        color: #2563EB;
        border: 2px solid rgba(255, 255, 255, 0.9);
        font-weight: 700;
        border-radius: 8px;
        box-shadow: 0 4px 16px rgba(0, 0, 0, 0.2);
      }
      
      .seasonal-badge {
        background: linear-gradient(135deg, #F59E0B, #D97706);
        color: white;
        padding: 4px 8px;
        border-radius: 10px;
        font-size: 10px;
        font-weight: 500;
        display: inline-block;
        margin-top: 5px;
      }
      
      .destination-stats {
        background: rgba(255, 255, 255, 0.05);
        backdrop-filter: blur(10px);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 8px;
        padding: 8px 12px;
        margin: 8px 0;
        font-size: 11px;
        color: rgba(255, 255, 255, 0.8);
      }
      
      /* Mobile responsiveness for carousel */
      @media screen and (max-width: 600px) {
        .carousel-row {
          flex-direction: column !important;
        }
        
        .carousel-item {
          margin: 0 0 15px 0 !important;
          width: 100% !important;
        }
        
        .mobile-carousel-stack {
          width: 100% !important;
          display: block !important;
        }
      }
      
      /* Scrolling indicators */
      .scroll-indicator {
        background: linear-gradient(90deg, transparent, rgba(37, 99, 235, 0.1), transparent);
        height: 2px;
        width: 100%;
        margin: 10px 0;
        border-radius: 1px;
        position: relative;
        overflow: hidden;
      }
      
      .scroll-progress {
        background: linear-gradient(90deg, #2563EB, #3B82F6);
        height: 100%;
        width: 33%;
        border-radius: 1px;
        animation: scroll-progress 3s ease-in-out infinite;
      }
      
      @keyframes scroll-progress {
        0% { transform: translateX(-100%); }
        50% { transform: translateX(200%); }
        100% { transform: translateX(-100%); }
      }
    </mj-style>
  </mj-head>
  
  <mj-body background-color="#F8FAFC" css-class="dark-mode-bg">
    
